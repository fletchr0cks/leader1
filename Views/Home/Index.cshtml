@{
    ViewBag.Title = "Home Page";
}




<div class="row">
    <div class="col-md-2" style="width:30%">
        <label class="control-label" for="date">Data Start</label>
        <input class="form-control" id="date-start" name="date-start" placeholder="01-February-2021" type="text" />
        <label class="control-label" for="date">From</label>
        <input class="form-control" id="date-from" name="date-from" placeholder="DD/MM/YYY" type="text" />
        <label class="control-label" for="date">To</label>
        <input class="form-control" id="date-to" name="date-to" placeholder="DD/MM/YYY" type="text" />
        <hr />
        <button type="button" class="btn btn-primary btn-lg">Get Data</button>
        <a href="#" class="btn btn-primary btn-lg" onclick="orderData()">Orders</a>
    </div>
    
    <div class="col-md-2" style="width:40%;visibility:hidden">
        <form>
